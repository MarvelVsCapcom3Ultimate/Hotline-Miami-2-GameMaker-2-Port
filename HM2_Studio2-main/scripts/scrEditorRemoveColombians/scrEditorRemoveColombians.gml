function scrEditorRemoveColombians() {
	with objEditorEnemy {
	if sprite_index=sprColombianWalkFamae instance_destroy()
	if sprite_index=sprColombianWalkShotgun instance_destroy()
	if sprite_index=sprColombianWalkSilencer instance_destroy()
	if sprite_index=sprColombianWalkMendoza instance_destroy()
	if sprite_index=sprColombianWalkMachete instance_destroy()
	if sprite_index=sprColombianWalkKnife instance_destroy()
	if sprite_index=sprColombianWalkPipe instance_destroy()
	}



}
